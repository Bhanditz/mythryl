## win32-file.pkg



# The implementation of the file stack on Win32 systems.

package file
:>
File
    where type pure_io::Reader = text_filereaders_and_filewriters::Reader
    where type pure_io::Writer = text_filereaders_and_filewriters::Writer
    where type pure_io::pos    = text_filereaders_and_filewriters::pos
=
    file_g (

        package winix_filereaders_and_filewriters
            =
            Win32TextPrimIO
    );



