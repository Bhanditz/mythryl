## win32-file.pkg



# The implementation of the file stack on Win32 systems.

package file
:>
File
    where type pure_io::Reader = text_streamreaders_and_streamwriters::Reader
    where type pure_io::Writer = text_streamreaders_and_streamwriters::Writer
    where type pure_io::pos    = text_streamreaders_and_streamwriters::pos
=
    file_g (

        package winix_streamreaders_and_streamwriters
            =
            Win32TextPrimIO
    );



