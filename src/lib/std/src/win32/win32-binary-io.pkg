## win32-binary-io.pkg



# The implementation of the binary_io stack on Win32 systems.


							# Binary_Io	is from   src/lib/std/src/io/binary-io.api
package binary_io :> Binary_Io
    where type pure_io::Reader = binary_filereaders_and_filewriters::Reader
    where type pure_io::Writer = binary_filereaders_and_filewriters::Writer
# *    where type pure_io::pos = binary_filereaders_and_filewriters::pos  *
=
binary_io_g (
    package winix_filereaders_and_filewriters = Win32BinPrimIO		# Win32BinPrimIO	is from   src/lib/std/src/win32/win32-binary-filereaders-and-filewriters.pkg
);




