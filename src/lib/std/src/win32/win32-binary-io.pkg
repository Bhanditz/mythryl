## win32-binary-io.pkg



# The implementation of the binary_io stack on Win32 systems.


							# Binary_Io	is from   src/lib/std/src/io/binary-io.api
package binary_io :> Binary_Io
    where type pure_io::Reader = binary_streamreaders_and_streamwriters::Reader
    where type pure_io::Writer = binary_streamreaders_and_streamwriters::Writer
# *    where type pure_io::pos = binary_streamreaders_and_streamwriters::pos  *
=
binary_io_g (
    package winix_streamreaders_and_streamwriters = Win32BinPrimIO
);




