## win32-binary-io.pkg



# The implementation of the binary_io stack on Win32 systems.


							# Binary_Io	is from   src/lib/std/src/io/binary-io.api
package binary_io :> Binary_Io
    where type pure_io::Reader = binary_file_io_driver_for_posix::Reader
    where type pure_io::Writer = binary_file_io_driver_for_posix::Writer
# *    where type pure_io::pos = binary_file_io_driver_for_posix::pos  *
=
binary_io_g (
    package winix_filereaders_and_filewriters = binary_file_io_driver_for_win32		# binary_file_io_driver_for_win32	is from   src/lib/std/src/win32/binary-file-io-driver-for-win32.pkg
);




