## win32-binary-io.pkg



# The implementation of the binary_io stack on Win32 systems.


							# Binary_Io	is from   src/lib/std/src/io/binary-io.api
package binary_io :> Binary_Io
    where type pure_io::Reader = binary_stream_readers_and_writers::Reader
    where type pure_io::Writer = binary_stream_readers_and_writers::Writer
# *    where type pure_io::pos = binary_stream_readers_and_writers::pos  *
=
binary_io_g (
    package winix_stream_readers_and_writers = Win32BinPrimIO
);




