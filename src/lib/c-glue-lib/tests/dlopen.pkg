package dl {

    local
	package ci = unsafe::mythryl_callable_c_library_interface;
    in
	my dlopen:   (Null_Or( String ), Bool, Bool) -> unt1::Unt
	    =
            ci::find_c_function
              {
                lib_name  => "dynamic_loading",
                fun_name =>  "dlopen"
              };

	my dlsym:    (unt1::Unt, String) -> unt1::Unt
	    =
            ci::find_c_function
              {
                lib_name  => "dynamic_loading",
                fun_name =>  "dlsym"
              };

        my dlerror:  Void -> Null_Or( String )
	    =
            ci::find_c_function
              {
                lib_name  => "dynamic_loading",
                fun_name =>  "dlerror"
              };

	my dlclose:  unt1::word -> Void
	    =
            ci::find_c_function
              {
                lib_name  => "dynamic_loading",
                fun_name =>  "dlclose"
              };
    end
}
