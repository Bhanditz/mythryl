			=> 
			{   fun print_one _ (typevar)
				=
				(prettyprint_typevar context pp (typevar, d));

			    uj::unparse_closed_sequence
				pp
				{ front      =>  (fn pp => pp::lit pp "("; end ),
				  separator  =>  { pp::lit pp ", ";fn pp => (pp::break pp { spaces=>1, indent_on_wrap=>0 } ); end ;},
				  back       =>  { pp::lit pp ")";fn pp => (pp::break pp { spaces=>1, indent_on_wrap=>0 } ); end ;},
				  print_one,
				  breakstyle =>  uj::ALIGN
				}
				tyvar_list;
			};
                end;

