	also
	fun prettyprint_pattern_clause (context as (_, source_opt)) pp
            =
	    {   ppsay =  pp::lit  pp;
		#
		fun prettyprint_pattern_clause' (rs::PATTERN_CLAUSE { patterns, result_type, expression }, d)
                    =
		    {   ppsay "rs::PATTERN_CLAUSE[ ";
			pp::newline pp;
			pp::open_box (pp, pp::typ::BOX_RELATIVE { spaces => 1, tab_to => 0, tabstops_are_every => 4 }, pp::vertical, 100 );
			pp::newline pp;

                        fun print_one _ {   item:		rs::Case_Pattern,
					    fixity:		Null_Or( rs::Symbol ),
					    source_code_region:	rs::Source_Code_Region
					}
                            =
			    case fixity
			        #
				THE a =>  prettyprint_pattern context pp (item, d);

				NULL => case item
					    #
					    rs::PRE_FIXITY_PATTERN p
						=>
						{   ppsay "rs::PRE_FIXITY_PATTERN ";
						    pp::lit pp "(";prettyprint_pattern context pp (item, d);
						    pp::lit pp ")";
						};

					    rs::TYPE_CONSTRAINT_PATTERN p
						=>
						{   ppsay "rs::TYPE_CONSTRAINT_PATTERN ";
						    pp::lit pp "(";prettyprint_pattern context pp (item, d);
						    pp::lit pp ")";
						};

					    rs::AS_PATTERN p
						=>
						{   ppsay "rs::AS_PATTERN ";
						    pp::lit pp"(";prettyprint_pattern context pp (item, d);
						    pp::lit pp ")";
						};

					    rs::OR_PATTERN p
						=>
						{   ppsay "rs::OR_PATTERN ";
						    pp::lit pp "(";prettyprint_pattern context pp (item, d);
						    pp::lit pp ")";
						};

					    _   =>
						prettyprint_pattern context pp (item, d);
					esac;
			    esac;


		    
			pp::shut_box pp;
			pp::open_box (pp, pp::typ::BOX_RELATIVE { spaces => 1, tab_to => 0, tabstops_are_every => 4 }, pp::vertical, 100 );
			pp::newline pp;

			uj::unparse_sequence
			    pp
			    { separator  =>  (fn pp =  (pp::break pp { spaces=>1, indent_on_wrap=>0 } )),
			      print_one,
			      breakstyle =>  uj::ALIGN
			    }
			    patterns;

			case result_type
			    #			  
			    THE type
				=>
				{   pp::lit pp ":";
				    prettyprint_type context pp (type, d);
				};

			    NULL => ();
			esac;

			pp::newline pp;
			pp::shut_box pp;
			pp::newline pp;
			pp::lit pp "= (PATTERN_CLAUSE) ";
			pp::open_box (pp, pp::typ::BOX_RELATIVE { spaces => 1, tab_to => 0, tabstops_are_every => 4 }, pp::vertical, 100 );
			pp::newline pp;
			pp::break pp { spaces=>1, indent_on_wrap=>0 }; 
			prettyprint_expression context pp (expression, d);
			pp::newline pp;
		        ppsay "]PATTERN_CLAUSE";
			pp::newline pp;
			pp::shut_box pp;
			pp::newline pp;
		    }; 

		prettyprint_pattern_clause';
	    }

