	also
	fun unparse_pattern_clause (context as (_, source_opt)) buf
            =
	    {   ppsay =  pp::string  buf;

		fun unparse_pattern_clause' (rs::PATTERN_CLAUSE { patterns, result_type, expression }, d)
                    =
		    {   fun print_one _ {   item:		    rs::Case_Pattern,
					    fixity:		    Null_Or( rs::Symbol ),
					    source_code_region:    rs::Source_Code_Region
					}
                            =
			    case fixity
			        #
			         THE a =>   unparse_pattern context buf (item, d);

				 NULL =>    case item
						#
						rs::PRE_FIXITY_PATTERN p
						    =>
						    {   pp::string buf "(";unparse_pattern context buf (item, d);
							pp::string buf ")";
						    };

						rs::TYPE_CONSTRAINT_PATTERN p
						    =>
						    {   pp::string buf "(";unparse_pattern context buf (item, d);
							pp::string buf ")";
						    };

						rs::AS_PATTERN p
						    =>
						    {   pp::string buf"(";unparse_pattern context buf (item, d);
							pp::string buf ")";
						    };

						rs::OR_PATTERN p
						    =>
						    {   pp::string buf "(";unparse_pattern context buf (item, d);
							pp::string buf ")";
						    };

						_   =>
						    unparse_pattern context buf (item, d);
					    esac;
			    esac;


		    
			pp::begin_wrap_box buf;

			uj::unparse_sequence
			    buf
			    { separator  =>  (fn buf = (pp::break buf { spaces=>1, indent_on_wrap=>0 } )),
			      print_one,
			      breakstyle =>  uj::ALIGN
			    }
			    patterns;

			case result_type
			    #			  
			    THE type => {   pp::string buf ":";
					    unparse_type context buf (type, d);
					};

			    NULL => ();
			esac;

			pp::string buf " =";
			pp::break buf { spaces=>1, indent_on_wrap=>0 }; 
			unparse_expression context buf (expression, d);
			pp::end_box buf;

		    }; 

	    
		unparse_pattern_clause';
	    }

