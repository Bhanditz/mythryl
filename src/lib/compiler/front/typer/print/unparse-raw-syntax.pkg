			unparse_sumtype context buf (t, d);
                end;
	    
		unparse_sumtype';
	    }

	also
	fun unparse_sumtype_right_hand_side (context as (_, source_opt)) buf
            =
	    {   ppsay =  pp::string buf;
		#
		fun unparse_sumtype_right_hand_side'(_, 0)
                        =>
                        ppsay "<sumtype_naming_right_hand_side>";

		    unparse_sumtype_right_hand_side' (rs::VALCONS const, d)
			=> 
			{   fun print_one buf   (symbol: rs::Symbol,   tv: Null_Or(rs::Any_Type))
				=
				case tv
				    #
				    THE a =>    {    uj::unparse_symbol buf symbol;
						     ppsay " ";	# Was " of "
						     unparse_type context buf (a, d);
						};

				    NULL =>   (uj::unparse_symbol buf symbol);
				esac;

			    uj::unparse_sequence
				buf
				{ separator   => (fn buf = { pp::string buf " |";
								pp::break buf { spaces=>1, indent_on_wrap=>0 };
							      }
						 ),
				  print_one,
				  breakstyle => uj::ALIGN
				}
				const;
			};

		    unparse_sumtype_right_hand_side' (rs::REPLICAS symlist, d)
			=> 
			uj::unparse_sequence
			    buf
			    { separator  =>  (fn buf
						 =
						 {   pp::string buf " |";
						     pp::break buf { spaces=>1, indent_on_wrap=>0 };
						 }
					     ),
			      print_one  =>  (fn buf =  fn symbol =  uj::unparse_symbol buf symbol),
			      breakstyle =>  uj::ALIGN
			    }
			    symlist;
                end;
	    
                unparse_sumtype_right_hand_side';
	    }

	also
	fun unparse_named_exception (context as (_, source_opt)) buf
            =
	    {   ppsay = pp::string buf;
		pp_symbol_list = pp_path buf;

		fun unparse_named_exception' (_, 0)
                        =>
                        ppsay "<Eb>";

		    unparse_named_exception' (   rs::NAMED_EXCEPTION {
                                                         exception_symbol => exn,
                                                         exception_type   => etype
                                                     },
                                                     d
                                                 )
			=> 
			case etype
			    #			  
			    THE a =>    {   pp::begin_align_box buf;
					    uj::unparse_symbol buf exn;
					    pp::string buf " =";
					    pp::break buf { spaces=>1, indent_on_wrap=>2 };
					    unparse_type context buf (a, d - 1);
					    pp::end_box buf;
					};

			    NULL =>     {   pp::begin_align_box buf;
					    uj::unparse_symbol buf exn; 
					    pp::end_box buf;
					};
			esac;


		    unparse_named_exception' ( rs::DUPLICATE_NAMED_EXCEPTION { exception_symbol=>exn, equal_to=>edef }, d)
			=> 
			# ASK MACQUEEN IF WE NEED TO PRINT EDEF XXX BUGGO FIXME 
			{   pp::begin_align_box buf;
			    uj::unparse_symbol buf exn;
			    pp::string buf " =";
			    pp::break buf { spaces=>1, indent_on_wrap=>2 };
			    pp_symbol_list (edef);
			    pp::end_box buf;
			};

		    unparse_named_exception' (rs::SOURCE_CODE_REGION_FOR_NAMED_EXCEPTION (t, r), d)
			=>
			unparse_named_exception context buf (t, d);
                end;
	    
		unparse_named_exception';
	    }

	also
	fun unparse_named_package (context as (_, source_opt)) buf
            =
	    {   ppsay = pp::string buf;

		fun unparse_named_package' (_, 0)
                        =>
                        ppsay "<rs::NAMED_PACKAGE>";

		    unparse_named_package' ( rs::NAMED_PACKAGE { name_symbol=>name, definition=>def, constraint, kind }, d)
			=> 
			{   ppsay case kind
                                      rs::PLAIN_PACKAGE  => "package ";
                                      rs::CLASS_PACKAGE  => "class ";
                                      rs::CLASS2_PACKAGE => "class2 ";
                                  esac;

			    pp::begin_align_box buf;
			    uj::unparse_symbol buf name;

                            unparse_package_cast context buf constraint d;

                            pp::string buf " =";
			    pp::break buf { spaces=>1, indent_on_wrap=>2 };
                            unparse_package_expression context buf (def, d - 1);
			    pp::end_box buf;
			};

		    unparse_named_package' (rs::SOURCE_CODE_REGION_FOR_NAMED_PACKAGE (t, r), d)
			=>
			unparse_named_package context buf (t, d);
                end;
	    
		unparse_named_package';
	    }

	also
	fun unparse_named_generic (context as (_, source_opt)) buf
            =
	    {   ppsay =  pp::string  buf;
		#
		fun unparse_named_generic' (_, 0)
                        =>
                        ppsay "<rs::NAMED_GENERIC>";

		    unparse_named_generic' (
                        rs::NAMED_GENERIC {
                            name_symbol => name,
                            definition => rs::GENERIC_DEFINITION { parameters, body, constraint }
                        },
                        d
                    )
			=>
			{   pp::begin_align_box buf;
			    #
			    uj::unparse_symbol buf name;

			    {   fun print_one buf (THE symbol, api_expression)
					=>
					{   ppsay "(";
					    uj::unparse_symbol buf symbol;
					    ppsay " : ";
					    unparse_api_expression context buf (api_expression, d);
					    ppsay ")";
					};

				    print_one buf (NULL, api_expression)
					=>
					{   ppsay "(";
					    unparse_api_expression context buf (api_expression, d);
					    ppsay ")";
					};
                                end;

				{   uj::unparse_sequence
					buf
					{ separator  =>  (fn buf = (pp::break buf { spaces=>1, indent_on_wrap=>0 } )),
					  print_one,
					  breakstyle =>  uj::ALIGN
					}
					parameters;



                                    unparse_package_cast  context  buf  constraint  d;


				    pp::nonbreakable_spaces buf 1;

				    ppsay "=";
                                    pp::break buf { spaces=>1, indent_on_wrap=>0 };

				    unparse_package_expression context buf (body, d);};
			    };
			    pp::end_box buf;
			};

		    unparse_named_generic' ( rs::NAMED_GENERIC { name_symbol=>name, definition=>def }, d)
			=>
			{   pp::begin_align_box buf;
			    uj::unparse_symbol buf name;
			    pp::string buf " =";
			    pp::break buf { spaces=>1, indent_on_wrap=>2 };
			    unparse_generic_expression context buf (def, d - 1);
			    pp::end_box buf;
			}; 

		    unparse_named_generic' (rs::SOURCE_CODE_REGION_FOR_NAMED_GENERIC (t, r), d)
			=>
			unparse_named_generic context buf (t, d);
                end;
	    
		unparse_named_generic';
	    }

	also
	fun unparse_generic_api_naming (context as (_, source_opt)) buf
            =
	    {   ppsay =  pp::string  buf;
		#
		fun unparse_generic_api_naming'(_, 0)
                        =>
                        ppsay "<rs::NAMED_GENERIC_API>";

		    unparse_generic_api_naming' (rs::NAMED_GENERIC_API { name_symbol=>name, definition=>def }, d)
			=> 
			{   pp::begin_align_box buf;
			    ppsay "funsig ";
                            uj::unparse_symbol buf name;
                            ppsay " =";
			    pp::break buf { spaces=>1, indent_on_wrap=>2 };
                            unparse_generic_api_expression context buf (def, d - 1);
			    pp::end_box buf;
			};

		    unparse_generic_api_naming' (rs::SOURCE_REGION_FOR_NAMED_GENERIC_API (t, r), d)
			=>
			unparse_generic_api_naming context buf (t, d);
                end;
	    
		unparse_generic_api_naming';
	    }

	also
	fun unparse_typevar (context as (_, source_opt)) buf
            =
	    {   ppsay = pp::string buf;
		#
		fun unparse_typevar' (_, 0) => ppsay "<typevar>";
		    unparse_typevar' (rs::TYPEVAR s, d) => (uj::unparse_symbol buf s); 
		    unparse_typevar' (rs::SOURCE_CODE_REGION_FOR_TYPEVAR (t, r), d) => unparse_typevar context buf (t, d);
                end;
	    
		unparse_typevar';
	    }

	also
	fun unparse_type (context as (dictionary, source_opt)) buf
            =			
	    {   ppsay =  pp::string buf;
		#
		fun unparse_type' (_, 0)
                        =>
                        ppsay "<type>";

		    unparse_type' (rs::TYPEVAR_TYPE t, d)
                        =>
                        (unparse_typevar context buf (t, d));

		    unparse_type' (rs::TYPE_TYPE (type, []), d)
			=>
			{   pp::begin_indented_align_box buf (pp::CURSOR_RELATIVE 1);
			    pp_path buf type;
			    pp::end_box buf;
			};

		    unparse_type' (rs::TYPE_TYPE (type, args), d)
			=> 
			{   pp::begin_indented_align_box  buf  (pp::CURSOR_RELATIVE 1);
			    #
			    case type
				#			      
				[type] =>   if (sy::eq (sy::make_type_symbol("->"), type))
						#				         
						case args

						     [dom, ran]
							 =>
							 {   unparse_type' (dom, d - 1);
							     ppsay " ->";
							     pp::break buf { spaces=>1, indent_on_wrap=>2 };
							     unparse_type' (ran, d - 1);
							 };

						     _   =>
							 err::impossible "wrong args for -> type";
						esac;
					    else
						uj::unparse_symbol  buf  type;
						ppsay "(";
						unparse_type_args  (args, d);
						ppsay ")";
					    fi;

				_ => {   pp_path  buf  type;
					 ppsay "(";
                                         unparse_type_args  (args, d);
					 ppsay ")";
				     };
                            esac;

			    pp::end_box buf;
			};

		    unparse_type' (rs::RECORD_TYPE s, d)
			=> 
			{   fun print_one buf (symbol: rs::Symbol, tv: rs::Any_Type)
				= 
				{   uj::unparse_symbol buf symbol;
				    ppsay ": ";
				    unparse_type context buf (tv, d);
				};

			    uj::unparse_closed_sequence
				buf
				{ front      =>  (by pp::string "{ "),
				  separator  =>  (fn buf = { pp::string buf ", ";
								pp::break buf { spaces=>1, indent_on_wrap=>0 };
							      }
						 ),
				  back       =>  (by pp::string "}"),
				  print_one,
				  breakstyle =>  uj::ALIGN
				}
				s;
			};

		    unparse_type' (rs::TUPLE_TYPE t, d)
			=> 
			{   fun print_one _ (tv: rs::Any_Type)
				=
				(unparse_type context buf (tv, d));

			    uj::unparse_closed_sequence 
				buf
				{ front      =>  (by pp::string "("),
				  separator  =>  (fn buf =  {   pp::string buf ", ";				# Was " *"
								   pp::break buf { spaces=>0, indent_on_wrap=>0 };
							       }
						 ),
				  back       =>  (by pp::string ")"),
				  print_one,
				  breakstyle =>  uj::ALIGN
				}
				t;
			};

		    unparse_type' (rs::SOURCE_CODE_REGION_FOR_TYPE (t, r), d)
			=>
			unparse_type context buf (t, d);
                end 

		also
		fun unparse_type_args ([], d)
                        =>
                        ();

		    unparse_type_args ( [type], d)
			=> 
			{   if (strength type <= 1)
			        #  
				pp::begin_indented_wrap_box buf (pp::CURSOR_RELATIVE 1);
				ppsay "("; 
				unparse_type' (type, d); 
				ppsay ")";
				pp::end_box buf;
			    else 
				unparse_type' (type, d);
			    fi;

			    pp::break buf { spaces => 0,  indent_on_wrap => 0 };
			};

		    unparse_type_args (tys, d)
			=>
			uj::unparse_closed_sequence
			    buf 
			    { front      =>  by pp::string "(",
			      separator  =>  fn buf =  { pp::string buf ", ";
							 pp::break buf { spaces=>0, indent_on_wrap=>0 };
						       },
			      back       =>  by pp::string ") ",
			      breakstyle =>  uj::ALIGN, 
			      print_one  =>  fn _ =  fn type =  unparse_type' (type, d)
			    }
			    tys;
                end; 
	    
                unparse_type';
	    };
    };					# package unparse_raw_syntax 
end;					# top-level local 








