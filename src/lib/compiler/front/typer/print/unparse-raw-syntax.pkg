		unparse_where_spec';
	    }

	also
	fun unparse_api_expression (context as (dictionary, source_opt)) buf
            =
	    {   ppsay = pp::string buf;
		#
		fun unparse_api_expression'(_, 0)
                        =>
                        ppsay "<api_expression>";

		    unparse_api_expression'(rs::API_BY_NAME s, d)
                        =>
                        uj::unparse_symbol  buf  s;

		    unparse_api_expression'(rs::API_WITH_WHERE_SPECS (an_api, wherel), d)
			=>
			{   unparse_api_expression' (an_api, d);
			    pp::break buf { spaces=>1, indent_on_wrap=>0 };

			    case an_api
				#			      
				rs::API_BY_NAME s
				    =>
				    uj::ppvlist buf (
					"where ",
					"also ",
					(fn buf =  fn r =  unparse_where_spec context buf (r, d - 1)),
					wherel
				    );

				rs::SOURCE_CODE_REGION_FOR_API (rs::API_BY_NAME s, r)
				    =>
				    uj::ppvlist buf (
					"where ",
					"also ",
					(fn buf =  fn r =  unparse_where_spec context buf (r, d - 1)),
					wherel
				    );

				_ =>
				    {   pp::newline buf;
					#
					uj::ppvlist buf (
					    "where ",
					    "also ",
					    (fn buf =  fn r =  unparse_where_spec context buf (r, d - 1)),
					    wherel
					);
				    };
			    esac;
			};

		    unparse_api_expression' (rs::API_DEFINITION [], d)
			=>	
			{   ppsay "api {";
			    pp::nonbreakable_spaces buf 1;
			    ppsay "};";
			};

		    unparse_api_expression' (rs::API_DEFINITION specl, d)
			=> 
			{   fun print_one buf speci
                                =
                                unparse_specification context buf (speci, d);

			    pp::newline buf; # XXX BUGGO TEST ONLY
			    ppsay "api {";

			    pp::begin_vertical_box buf;
			    pp::newline buf;
#			    uj::newline_indent buf 4;

			    uj::unparse_sequence
				buf
				{ separator   =>  (fn buf =  pp::newline buf),
				  print_one,
				  breakstyle =>  uj::ALIGN
				}
				specl;

			    pp::end_box buf;

			    pp::newline buf;
			    ppsay "};";
			};

		    unparse_api_expression' (rs::SOURCE_CODE_REGION_FOR_API (m, r), d)
                        =>
                        unparse_api_expression context buf (m, d);
                end;
	    
		unparse_api_expression';
	    }

	also
	fun unparse_generic_api_expression (context as (dictionary, source_opt)) buf
            =
	    {   ppsay =  pp::string buf;
		#
		fun unparse_generic_api_expression'(_, 0)
                        =>
                        ppsay "<generic_api_expression>";

		    unparse_generic_api_expression' (rs::GENERIC_API_BY_NAME s, d)
                        =>
                        uj::unparse_symbol buf s;

		    unparse_generic_api_expression' (rs::GENERIC_API_DEFINITION { parameter, result }, d)
			=>
			{   fun print_one buf (THE symbol, api_expression)
				    =>
				    {   ppsay "(";
                                        uj::unparse_symbol buf symbol;
                                        ppsay ":";
					unparse_api_expression context buf (api_expression, d);
					ppsay ")";
				    };

			        print_one buf (NULL, api_expression)
				    =>
				    {   ppsay "("; unparse_api_expression context buf (api_expression, d);
                                        ppsay ")";
                                    };
                            end;

			    uj::unparse_sequence
				buf
				{ separator  => (fn buf =  (pp::newline buf)),
				  print_one,
				  breakstyle => uj::ALIGN
				}
				parameter;

			    pp::break buf { spaces=>1, indent_on_wrap=>2 };
			    ppsay "=> ";
			    unparse_api_expression context buf (result, d);
			};

		    unparse_generic_api_expression' (rs::SOURCE_CODE_REGION_FOR_GENERIC_API (m, r), d)
			=>
			unparse_generic_api_expression context buf (m, d);
                end;
	    
		unparse_generic_api_expression';
	    }

	also
	fun unparse_specification (context as (dictionary, source_opt)) buf
            =
	    {   ppsay =  pp::string  buf;
		#
		fun pp_tyvar_list ([], d)
                        =>
                        ();

		    pp_tyvar_list ( [typevar], d)
			=> 
			{   unparse_typevar context buf (typevar, d);
			    pp::break buf { spaces=>1, indent_on_wrap=>0 };
			};

		    pp_tyvar_list (tyvar_list, d)
			=> 
			{   fun print_one _ typevar
				=
				(unparse_typevar context buf (typevar, d));

			    uj::unparse_closed_sequence
				buf
				{ front      =>  (fn buf = pp::string buf "("),
				  separator  =>  { pp::string buf ", "; fn buf = (pp::break buf { spaces=>1, indent_on_wrap=>0 } ); },
				  back       =>  { pp::string buf ")" ; fn buf = (pp::break buf { spaces=>1, indent_on_wrap=>0 } ); },
				  print_one,
				  breakstyle =>  uj::ALIGN
				}
				tyvar_list;
			};
                end;

		fun unparse_specification'(_, 0)
                        =>
                        ppsay "<Specification>";

		    unparse_specification' (rs::PACKAGES_IN_API sspo_list, d)
			=>
			{   fun print_one _ (symbol, api_expression, path)
				=
				case path
				    #
				    THE p => {   uj::unparse_symbol buf symbol;
						 ppsay " = ";
						 unparse_api_expression context buf (api_expression, d);
						 pp::break buf { spaces=>1, indent_on_wrap=>0 };
						 pp_path buf p;
					     };

				    NULL  => {   uj::unparse_symbol buf symbol;
						 ppsay " = ";
						 unparse_api_expression context buf (api_expression, d);
					     };
				esac;

			    uj::unparse_closed_sequence
				buf
				{ front      =>  (by pp::string "packageY "),
				  separator  =>  (fn buf
						     =
						     {   pp::string buf ", ";
							 pp::break buf { spaces=>1, indent_on_wrap=>0 };
						     }
						 ),
				  back       =>  (by pp::string ""),
				  print_one,
				  breakstyle =>  uj::ALIGN
				}
				sspo_list;
			}; 

		    unparse_specification' (rs::TYPES_IN_API (stto_list, bool), d)
			=> 
			{   fun print_one _ (symbol, tyvar_list, tyo)
				=
				case tyo
				    #
				    THE type
					=>
					{   uj::unparse_symbol buf symbol;
					    ppsay "(";
					    pp_tyvar_list (tyvar_list, d);
					    ppsay ") =  ";
					    unparse_type context buf (type, d);
					};

				    NULL
					=>
					{   uj::unparse_symbol buf symbol;
					    ppsay "(";
					    pp_tyvar_list (tyvar_list, d);
					    ppsay ")";
					};
				esac;


			    uj::unparse_closed_sequence
				buf
				{ front      =>  (by pp::string ""),			# Was "type "
				  separator  =>  fn buf =  {   pp::string buf "|";
							     pp::newline buf;
							 },
				  back       =>  (by pp::string ";"),
				  print_one,
				  breakstyle =>  uj::ALIGN
				}
				stto_list;
			}; 

		    unparse_specification' (rs::GENERICS_IN_API sf_list, d)
			=>
			{   fun pr buf (symbol, generic_api_expression)
				=
				{   uj::unparse_symbol buf symbol;
                                    ppsay " : ";
				    unparse_generic_api_expression context buf (generic_api_expression, d - 1);
				}; 

			    pp::begin_align_box buf;
			    uj::ppvlist buf ("generic package ", "also ", pr, sf_list);
			    pp::end_box buf;
			}; 

		    unparse_specification' (rs::VALUES_IN_API st_list, d)
			=> 
			{   fun pr buf (symbol, type)
				= 
				{   uj::unparse_symbol buf symbol;
                                    ppsay ":   ";
                                    unparse_type context buf (type, d);
                                }; 

			    pp::begin_align_box buf;
			    uj::ppvlist buf (
                                "",		# Was "my ",
                                "also ",
                                pr, 
                                st_list
                            );
                            ppsay "; ";
			    pp::end_box buf;
			}; 

		    unparse_specification' (rs::VALCONS_IN_API { sumtypes, with_types => [] }, d)
			=> 
			{   fun pr buf (dbing)
				=
				(unparse_sumtype context buf (dbing, d));

			    pp::begin_align_box buf;
			    uj::ppvlist buf ("", "also ", pr, sumtypes);
			    pp::end_box buf;
			}; 

		    unparse_specification' (rs::VALCONS_IN_API { sumtypes, with_types }, d)
			=> 
			{   fun prd buf (dbing) = (unparse_sumtype context buf (dbing, d));
			    fun prw buf (tbing) = (unparse_named_type context buf (tbing, d));

			    {   pp::begin_align_box buf;
				uj::ppvlist buf ("", "also ", prd, sumtypes);
				pp::newline buf;
				uj::ppvlist buf ("", "also ", prw, with_types);
				pp::end_box buf;
			    };
			};

		    unparse_specification' (rs::EXCEPTIONS_IN_API sto_list, d)
			=> 
			{   fun pr buf (symbol, tyo)
				=
				case tyo
				    #
				    THE type => {   uj::unparse_symbol buf symbol;
						    ppsay " : ";
						    unparse_type context buf (type, d);
						};

				    NULL => uj::unparse_symbol buf symbol;
				esac;

			    pp::begin_align_box buf;
			    uj::ppvlist buf ("exception ", "also ", pr, sto_list);
			    pp::end_box buf;
			}; 

		    unparse_specification' (rs::PACKAGE_SHARING_IN_API paths, d)
			=> 
			{   pp::begin_align_box buf;
			    uj::ppvlist buf ("sharing ", " = ", pp_path, paths);
			    pp::end_box buf;
			};

		    unparse_specification' (rs::TYPE_SHARING_IN_API paths, d)
			=> 
			{   pp::begin_align_box buf;
			    uj::ppvlist buf ("sharing ", " = ", pp_path, paths);
			    pp::end_box buf;
			};

		    unparse_specification' (rs::IMPORT_IN_API api_expression, d)
			=>
			unparse_api_expression context buf (api_expression, d);

		    unparse_specification' (rs::SOURCE_CODE_REGION_FOR_API_ELEMENT (m, r), d)
			=>
			unparse_specification context buf (m, d);
                end;
	    
		unparse_specification';
	    }

	also
	fun unparse_declaration (context as (dictionary, source_opt)) buf
            =
	    {   ppsay = pp::string buf;
		#
		pp_symbol_list =  pp_path  buf;

		fun unparse_declaration' (_, 0)
                        =>
                        ppsay "<declaration>";

		    unparse_declaration' (rs::VALUE_DECLARATIONS (vbs, typevars), d)
			=>
			{   pp::begin_align_box buf;
			    uj::ppvlist buf (
                                "my ",
                                "also ",
                                (fn buf =  fn named_value =  unparse_named_value context buf (named_value, d - 1)),
                                vbs
                            );
			    pp::end_box buf;
			};

		    unparse_declaration' (rs::FIELD_DECLARATIONS (fields, typevars), d)
			=>
			# 2009-02-23 CrT: A quick first-cut solution, duplicated from VALUE_DECLARATIONS: case:
			#
			{   pp::begin_align_box buf;
			    uj::ppvlist buf (
                                "field ",
                                "also ",
                                (fn buf =  fn named_value =  unparse_named_field context buf (named_value, d - 1)),
                                fields
                            );
			    pp::end_box buf;
			};

		    unparse_declaration' (rs::RECURSIVE_VALUE_DECLARATIONS (rvbs, typevars), d)
			=> 
			{   pp::begin_align_box buf;
			    uj::ppvlist
				buf
				(   "my rec ",
				    "also ",
				    (  fn buf =
				       fn named_recursive_values =
				       unparse_named_recursive_values
					   context
					   buf
					   (named_recursive_values, d - 1)
				    ),
				    rvbs
				);

			    pp::end_box buf;
			};

		    unparse_declaration' (rs::FUNCTION_DECLARATIONS (fbs, typevars), d)
			=>
			{   pp::begin_align_box buf;
			    uj::ppvlist'
				buf
				(   "fun ",
				    "also ",
				    (   fn buf =
					fn str =
					fn fb =
					unparse_named_sml_function
					    context
					    buf
					    str
					    (fb, d - 1)
				    ),
				    fbs
				);

			    pp::end_box  buf;
			};

		    unparse_declaration' (rs::NADA_FUNCTION_DECLARATIONS (fbs, typevars), d)
			=>
			{   pp::begin_align_box buf;
			    uj::ppvlist'
				buf
				(   "fun ",
				    "also ",
				    (   fn buf =
					fn str =
					fn fb =
					unparse_named_lib7function
					    context
					    buf
					    str
					    (fb, d - 1)
				    ),
				    fbs
				);

			    pp::end_box  buf;
			};

		    unparse_declaration' (rs::TYPE_DECLARATIONS types, d)
			=>
			{   fun print_one  buf  type
				=
				(unparse_named_type context buf (type, d));

			    uj::unparse_closed_sequence
				buf
				{ front      =>  (by pp::string ""),	# Was "type "
				  separator  =>  (fn buf =  (pp::break buf { spaces=>1, indent_on_wrap=>0 } )),
				  back       =>  (by pp::string ";"),
				  print_one,
				  breakstyle =>  uj::ALIGN
				}
				types;
			}; 	

		    unparse_declaration' (rs::SUMTYPE_DECLARATIONS { sumtypes, with_types => [] }, d)
			=> 
			{   fun print_one _ (dbing)
				=
				(unparse_sumtype context buf (dbing, d));


			    uj::unparse_closed_sequence
				buf
				{ front      =>  (by pp::string ""),
				  separator  =>  (fn buf = (pp::break buf { spaces=>1, indent_on_wrap=>0 } )),
				  back       =>  (by pp::string ";"),
				  print_one,
				  breakstyle =>  uj::ALIGN
				}
				sumtypes;
			};             

		    unparse_declaration' (rs::SUMTYPE_DECLARATIONS { sumtypes, with_types }, d)
			=> 
			{   fun prd buf dbing = (unparse_sumtype context buf (dbing, d));
			    fun prw buf tbing = (unparse_named_type context buf (tbing, d));

			    {   pp::begin_align_box buf;
				#
				uj::unparse_closed_sequence
				    buf
				    { front      =>  (by pp::string ""),
				      separator  =>  (fn buf = (pp::break buf { spaces=>1, indent_on_wrap=>0 } )),
				      back       =>  (by pp::string ";"),
				      print_one  =>  prd,
				      breakstyle =>  uj::ALIGN
				    }
				    sumtypes;

				pp::newline buf;

				uj::unparse_closed_sequence
				    buf
				    { front      =>  (by pp::string "withtype "),
				      separator  =>  (fn buf = (pp::break buf { spaces=>1, indent_on_wrap=>0 } )),
				      back       =>  (by pp::string ""),
				      print_one  =>  prw,
				      breakstyle =>  uj::ALIGN
				    }
				    with_types;

				pp::end_box buf;
			    };
			};

		    unparse_declaration' (rs::ABSTRACT_TYPE_DECLARATIONS { abstract_types, with_types => [], body }, d)
			=> 
			{   fun prd buf dbing = (unparse_sumtype context buf (dbing, d));
			    fun prw buf tbing = (unparse_named_type     context buf (tbing, d));

			    {   pp::begin_align_box buf;
				(   uj::unparse_closed_sequence
					buf
					{ front      =>  (by pp::string ""),
					  separator  =>  (fn buf = (pp::break buf { spaces=>1, indent_on_wrap=>0 } )),
					  back       =>  (by pp::string ";"),
					  print_one  =>  prd,
					  breakstyle =>  uj::ALIGN
					}
					abstract_types
				);
				pp::newline buf;
				unparse_declaration' (body, d);
				pp::end_box buf;
			    };
			};

		    unparse_declaration' (rs::ABSTRACT_TYPE_DECLARATIONS { abstract_types, with_types, body }, d)
			=> 
			{   fun prd _ (dbing) = (unparse_sumtype context buf (dbing, d));
			    fun prw _ (tbing) = (unparse_named_type context buf (tbing, d));

			    {   pp::begin_align_box buf;
				(   uj::unparse_closed_sequence
					buf
					{ front      =>  (by pp::string ""),
					  separator  =>  (fn buf = (pp::break buf { spaces=>1, indent_on_wrap=>0 } )),
					  back       =>  (by pp::string ";"),
					  print_one  =>  prd,
					  breakstyle =>  uj::ALIGN
					}
					abstract_types
				);
				pp::newline buf;
				(   uj::unparse_closed_sequence
					buf
					{ front      =>  (by pp::string "withtype "),
					  separator  =>  (fn buf = (pp::break buf { spaces=>1, indent_on_wrap=>0 } )),
					  back       =>  (by pp::string ""),
					  print_one  =>  prw,
					  breakstyle =>  uj::ALIGN
					}
					with_types
				);
				pp::newline buf;
				unparse_declaration' (body, d);
				pp::end_box buf;
			    };
			};

		    unparse_declaration' (rs::EXCEPTION_DECLARATIONS ebs, d)
			=>
			{   pp::begin_align_box buf;
			    (   (fn buf =  fn eb =  unparse_named_exception context buf (eb, d - 1)),   ebs   );
			    pp::end_box buf;
			};

		    unparse_declaration' (rs::PACKAGE_DECLARATIONS sbs, d)
			=>
			{   fun print_one _ sbing
				=
				(unparse_named_package context buf (sbing, d));

			    uj::unparse_closed_sequence
				buf
				{   front      =>  (by pp::string "package "),
				    separator  =>  (fn buf =  (pp::break buf { spaces=>1, indent_on_wrap=>0 } )),
				    back       =>  (by pp::string ";"),
				    print_one,
				    breakstyle =>  uj::ALIGN
				}
				sbs;
			};

		    unparse_declaration' (rs::GENERIC_DECLARATIONS fbs, d)
			=> 
			{   fun f buf generic_naming
				=
				unparse_named_generic context buf (generic_naming, d);


			    pp::begin_align_box buf;
			    uj::ppvlist buf ("generic package ", "also ", f, fbs);
			    pp::end_box buf;
			};

		    unparse_declaration' (rs::API_DECLARATIONS sigvars, d)
			=> 
			{   fun f buf (rs::NAMED_API { name_symbol=>fname, definition=>def } )
				=>
				{   uj::unparse_symbol  buf  fname;
				    pp::newline buf;
                                    ppsay "=";
				    unparse_api_expression context buf (def, d);
				};

				f buf (rs::SOURCE_CODE_REGION_FOR_NAMED_API (t, r))
                                    =>
                                    f buf t;
			    end;


			    pp::begin_align_box buf;
			    uj::ppvlist buf ("api ", "also ", f, sigvars);	# Was "api "
			    pp::end_box buf;
			};

		    unparse_declaration' (rs::GENERIC_API_DECLARATIONS sigvars, d)
			=> 
			{   fun print_one buf sigv
				=
				unparse_generic_api_naming context buf (sigv, d);

			    pp::begin_align_box buf;

			    uj::unparse_sequence
				buf
				{ separator  =>  pp::newline,
				  print_one,
				  breakstyle =>  uj::ALIGN
				}
				sigvars;

			    pp::end_box buf;
			};

		    unparse_declaration' (rs::LOCAL_DECLARATIONS (inner, outer), d)
			=>
			{   pp::align_box buf .{
				pp::newline buf;  ppsay "with";
                                pp::vertical_box buf .{
				    pp::newline buf;  unparse_declaration'(inner, d - 1);
                                };
				pp::newline buf;  ppsay "do ";
                                pp::vertical_box buf .{
				    pp::newline buf;  unparse_declaration'(outer, d - 1);
	                        };
				pp::newline buf;  ppsay "end;\t\t# with";
			    };
			    pp::newline buf;
			};

		    unparse_declaration' (rs::SEQUENTIAL_DECLARATIONS decs, d)
			=>
			{   pp::begin_align_box buf;
			    #
			    uj::unparse_sequence
				buf
				{ separator  =>  pp::newline,
				  print_one  =>  (fn buf =  fn declaration =  unparse_declaration' (declaration, d)),
				  breakstyle =>  uj::ALIGN
				}
				decs;

			    pp::end_box buf;
			};

		    unparse_declaration' (rs::INCLUDE_DECLARATIONS named_packages, d)
			=> 
			{   pp::begin_align_box buf;
			    ppsay "use ";

			    uj::unparse_sequence
				buf
				{ separator  =>  (fn buf =  pp::break buf { spaces=>1, indent_on_wrap=>0 }),
				  print_one  =>  (fn buf =  fn sp =  pp_symbol_list sp),
				  breakstyle =>  uj::ALIGN
				}
				named_packages;

			    pp::end_box buf;
			};

		    unparse_declaration' (rs::OVERLOADED_VARIABLE_DECLARATION (symbol, type, explist, extension), d)
		        =>
			{   ppsay "overloaded my ";
		            uj::unparse_symbol buf symbol;
			    ppsay ( extension ?? " += ... " :: " = ... ");
			};

		    unparse_declaration' (rs::FIXITY_DECLARATIONS { fixity, ops }, d)
			=>
			{   pp::begin_align_box buf;
			    #
			    case fixity
                                #
				fxt::NONFIX => ppsay "nonfix my ";

				fxt::INFIX (i, _)
				 => 
				 {   if (i % 2 == 0)	ppsay "infix my ";
				     else		ppsay "infixr my ";
                                     fi;

				     if (i / 2 > 0)
				         #
					 ppsay (int::to_string (i / 2));
					 ppsay " ";
				     fi;
				 };
			   esac;

			   uj::unparse_sequence
			       buf
			       { separator  =>  (fn buf =  pp::break buf { spaces=>1, indent_on_wrap=>0 }),
				 print_one  =>  uj::unparse_symbol,
				 breakstyle =>  uj::ALIGN
			       }
			       ops;

			   pp::end_box buf;
			};

		    unparse_declaration' (rs::SOURCE_CODE_REGION_FOR_DECLARATION (declaration, (s, e)), d)
			=>  
#			case source_opt
#			    #
#			    THE source
#				 =>
#				 {   ppsay "rs::SOURCE_CODE_REGION_FOR_DECLARATION(";
#				     unparse_declaration'(declaration, d); ppsay ", ";
#				     prpos (buf, source, s); ppsay ", ";
#				     prpos (buf, source, e); ppsay ")";
#				 };
#
#			    NULL
#			       =>
			       unparse_declaration' (declaration, d);
#			esac;

		    unparse_declaration' (rs::PRE_COMPILE_CODE string, d)
			=>
			ppsay ("#DO " + string);

                  end;
	      
                  unparse_declaration';
	      }

