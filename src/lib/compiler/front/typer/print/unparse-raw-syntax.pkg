			    ppsay "packageZ ";

			    uj::unparse_sequence
				buf
				{ separator =>  (fn buf =  (pp::break buf { spaces=>1, indent_on_wrap=>0 } )),
				  print_one,
				  breakstyle =>  uj::ALIGN
				}
				slist;

			    pp::break buf { spaces=>1, indent_on_wrap=>0 };

			    uj::unparse_sequence
				buf
				{ separator  =>  (fn buf =  (pp::break buf { spaces=>1, indent_on_wrap=>0 } )),
				  print_one,
				  breakstyle =>  uj::ALIGN
				}
				slist';
			};
