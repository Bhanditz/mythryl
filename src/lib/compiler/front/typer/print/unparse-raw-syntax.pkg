	also
	fun unparse_named_sml_function (context as (_, source_opt)) buf head
            = 
	    {   ppsay =  pp::string  buf;


		fun unparse_named_sml_function'(_, 0)
                        =>
                        ppsay "<FunNaming>";

		    unparse_named_sml_function' (rs::NAMED_FUNCTION { pattern_clauses, is_lazy, kind, null_or_type }, d)
			=>
			{
			    case kind
				rs::PLAIN_FUN => ppsay "";
			       rs::METHOD_FUN => ppsay " (method) ";
			      rs::MESSAGE_FUN => ppsay " (message) ";
			    esac;

                            case null_or_type 
				THE type => {   ppsay " : ";
						unparse_type context buf (type, d - 1);
					    };
				NULL => ();
			    esac;

			    uj::ppvlist buf
                              ( head, "  ; ",
			        (fn buf =   fn (cl: rs::Pattern_Clause) =   (unparse_pattern_clause context buf (cl, d))),
			        pattern_clauses
                              );
			};

		    unparse_named_sml_function' (rs::SOURCE_CODE_REGION_FOR_NAMED_FUNCTION (t, r), d)
			=>
			unparse_named_sml_function context buf head (t, d);
