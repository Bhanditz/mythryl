## packed-widget-egg.pkg
#
# This defines the egg interface which all widgets implement
# for the gui control system.

# Compiled by:
#     src/lib/x-kit/widget/xkit-widget.sublib



stipulate
    include threadkit;										# threadkit				is from   src/lib/src/lib/thread-kit/src/core-thread-kit/threadkit.pkg
    #
    package g2l =  gui_to_look;									# gui_to_look				is from   src/lib/x-kit/widget/gui/gui-to-look.pkg
    package l2g =  look_to_gui;									# look_to_gui				is from   src/lib/x-kit/widget/gui/look-to-gui.pkg
    package pwc =  packed_widget_control;							# packed_widget_control			is from   src/lib/x-kit/widget/space/packed/packed-widget-control.pkg
herein

    # This port is implemented in:
    #
    #     
    #
    package packed_widget_egg {
	#
	Exports	= {										# Ports we provide for use by other imps.
		    packed_widget_control:	pwc::Packed_Widget_Control
		  };

	Imports = {										# Ports we use, provided by other imps.
		    look_to_gui:		l2g::Look_To_Gui,				# We use this to get the attention of   src/lib/x-kit/widget/gui/gui-boss-imp.pkg
                    int_sink:			Int -> Void
                  };

	Packed_Widget_Egg =  Void -> (Exports,   (Imports, Run_Gun, End_Gun) -> Void);
    };												# package packed_widget_egg
end;




