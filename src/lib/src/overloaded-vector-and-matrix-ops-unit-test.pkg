	    =
	    {
		printf "\nDoing %s:\n" name;   

		exercise_vector_gets_and_sets ();

		exercise_rw_matrix_gets_and_sets_of_chars   ();
		exercise_rw_matrix_gets_and_sets_of_strings ();
		exercise_rw_matrix_gets_and_sets_of_ints    ();
		exercise_rw_matrix_gets_and_sets_of_floats  ();

		summarize_unit_tests  name;
	    };
    };
