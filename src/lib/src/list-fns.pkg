    fun make_list (len, genfn)
	=
	if (len < 0)
	    #
	    lib_base::failure { module=>"list_fns", func=>"make_list", msg=>"len < 0"};
        else
            loop (len - 1,[])
	    where
                fun loop (i, l)
		    =
		    if (i < 0)      l;
                    else            loop (i - 1, (genfn i) ! l);
		    fi;
            end;
	fi;

       
};					# package list_fns



